<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_magento_product_url_form" model="ir.ui.view">
			<field name="name">magento.product.url.form</field>
			<field name="model">magento.product.url</field>
			<field name="arch" type="xml">
				<form string="Product url" create="false">
					<sheet>
						<notebook>
							<page string="Product Url" name="product_url">
								<group>
									<field name="magento_product_id" />
									<field name="website_id" />
									<field name="storeview_id" />
									<field name="product_url" />
								</group>
							</page>
						</notebook>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</odoo>
        